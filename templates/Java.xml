<templateSet group="Java">
  <template name="ma" value="/**&#10; * $params$&#10; * $return$&#10; * $user$ $date$ $time$&#10; */" description="在方法的内部输入ma时，会在方法外生成注释。" toReformat="false" toShortenFQNames="true">
    <variable name="params" expression="methodParameters()" defaultValue="" alwaysStopAt="true" />
    <variable name="return" expression="methodReturnType()" defaultValue="" alwaysStopAt="true" />
    <variable name="user" expression="user()" defaultValue="" alwaysStopAt="true" />
    <variable name="date" expression="date()" defaultValue="" alwaysStopAt="true" />
    <variable name="time" expression="time()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="ca" value="/*--------------------------------- 业务处理 ---------------------------------*/&#10;&#10;&#10;/*--------------------------------- 方法返回 ---------------------------------*/&#10;" description="Controller注释" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>